<!DOCTYPE html>
<html lang="en">
<%- include('includes/head') %>
<body>
   <%- include('includes/header') %>
    
    <main class="suggestions">
        <h1>SUGGESTIONS</h1>
        <ul>
        <% posts.forEach(post => { %>
            <li><a href="/post/<%= post.id %>"><%= post.title %></a> posted by <%= post.username %> on <%= new Date(post.createdDate).getMonth() + 1 %>/<%= new Date(post.createdDate).getMonth() %>/<%= new Date(post.createdDate).getFullYear() %>  </li>
        <% }) %>
        </ul>
        
    </main>

    <%- include('includes/footer') %>
</body>
</html>